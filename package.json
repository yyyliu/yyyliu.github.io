{
  "name": "personal-homepage",
  "version": "1.0.0",
  "description": "My personal homepage.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yyyliu/yyyliu.github.io"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run prebuild && npm run webpack-prod",
    "dev": "npm run prebuild && npm run webpack-dev",
    "prebuild": "rm -rf build && mkdir build && cp -R assets/ build",
    "webpack-dev": "webpack --config webpack.dev.js",
    "webpack-prod": "webpack --progress --hide-modules --config webpack.prod.js",
    "start": "python3 -m http.server 8080"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^5.0.2",
    "jquery": "^3.4.1",
    "style-loader": "^0.20.3",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  }
}
